   "source": [
    "nums=[23,45,1,2,8,19,-3,16,-11,28]\n",
    "target=19\n",
    "\n",
    "\n",
    "def linear_search(nums,target):\n",
    "    if len(nums)==0:\n",
    "        return -1\n",
    "    for i in range(len(nums)):\n",
    "        if nums[i]==target:\n",
    "            return i\n",
    "    return -1\n",
    "\n",
    "print(linear_search(nums,target))\n",
    "print(linear_search(nums,target=111))\n",
    "\n"
   ]
